#!/bin/bash

while getopts "g:t:h:v:s:" opt; do
    case ${opt} in
        s)
            throttlectl "${OPTARG}" &
        ;;
        t)
            throttlectl turbo "${OPTARG}" &
        ;;
        h)
            throttlectl ht "${OPTARG}" &
        ;;
        g)
            throttlectl gov "${OPTARG}" &
        ;;
        v)
            throttlectl gpu "${OPTARG}" &
        ;;
    esac
done
